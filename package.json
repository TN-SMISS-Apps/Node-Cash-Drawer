{
  "name": "node-cash-drawer",
  "version": "1.0.1",
  "description": "node cash drawer middleware",
  "main": "dist/electron.js",
  "productName": "node-cash-drawer",
  "bin": "./dist/index.js",
  "scripts": {
    "start": "npm run copy & electron .",
    "watch": "tsc -w",
    "compile": "tsc",
    "copy": "copyfiles -f src/templates/* dist/templates",
    "build": "electron-packager . --out release --asar --platfrom=win32 --arch=x64 --prune=true --overwrite",
    "build:darwin": "electron-packager . --out release --asar --platfrom=darwin --prune=true --overwrite",
    "build:prod": "rimraf exes && npm run copy && npm run compile && npm run build && node src/build-utils/build-exe.js",
    "postinstall": "electron-rebuild"
  },
  "keywords": [],
  "author": "Telenorma",
  "license": "ISC",
  "dependencies": {
    "@hapi/joi": "^17.1.0",
    "body-parser": "^1.19.0",
    "copyfiles": "^2.3.0",
    "electron-log": "^4.3.5",
    "electron-store": "^6.0.0",
    "express": "^4.17.1",
    "ffi-napi": "^4.0.3",
    "multer": "^1.4.2",
    "serialport": "^9.0.1",
    "tcp-port-used": "^1.0.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/electron": "^1.6.10",
    "@types/electron-store": "^3.2.0",
    "@types/express": "^4.17.3",
    "@types/ffi-napi": "^4.0.3",
    "@types/hapi__joi": "^16.0.12",
    "@types/multer": "^1.4.4",
    "@types/node": "^12.12.55",
    "@types/serialport": "^8.0.1",
    "@types/tcp-port-used": "^1.0.0",
    "dot-prop": ">=5.1.1",
    "electron": "^9.3.0",
    "electron-packager": "^15.1.0",
    "electron-rebuild": "^2.0.3",
    "electron-winstaller": "^4.0.1",
    "nodemon": "^2.0.2",
    "ts-node": "^8.6.2",
    "typescript": "^3.9.7"
  }
}
